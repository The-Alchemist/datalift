@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix datalift: <http://www.datalift.org/core#> .

_:1 a datalift:SparqlQuery ;
    rdfs:label
        "All entries" ,
        "Toutes les entrées"@fr ;
    dcterms:description
        "Lists all entries (subject, predicates and objects) from the repository" ,
        "Retourne toutes les entrées (sujets, prédicats et objets) du dépôt"@fr ;
    rdf:value
        "SELECT * WHERE {\n  ?s ?p ?o .\n}\n" .

_:2 a datalift:SparqlQuery ;
    rdfs:label
        "Subjects" ,
        "Sujets"@fr ;
    dcterms:description
        "Lists all subjects in the repository" ,
        "Retourne la liste des sujets du dépôt"@fr ;
    rdf:value
        "SELECT DISTINCT ?s WHERE {\n  ?s ?p ?o .\n}\n" .

_:3 a datalift:SparqlQuery ;
    rdfs:label
        "Types" ;
    dcterms:description
        "Lists the RDF types present in the repository" ,
        "Retourne la liste des types RDF connus dans le dépôt"@fr ;
    rdf:value
        "SELECT DISTINCT ?type WHERE {\n  ?s a ?type .\n}\n" .

_:4 a datalift:SparqlQuery ;
    rdfs:label
        "Contexts" ,
        "Contextes"@fr ;
    dcterms:description
        "Lists the contexts (a.k.a named graphs) present in the repository" ,
        "Retourne la liste des contextes (graphes nommés) définis dans le dépôt"@fr ;
    rdf:value
        "SELECT DISTINCT ?graph WHERE {\n  GRAPH ?graph {\n    ?s ?p ?o .\n  }\n}\n" .

