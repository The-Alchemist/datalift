@prefix tpl: <http://www.mondeca.com/sesame/toolkit/template#> .


_:template a tpl:Template ;
	tpl:body "DELETE { ?s ?source ?o } INSERT { ?s ?target ?o } WHERE { ?s ?source ?o }" ;
	tpl:displayName "Move predicate to another" ;
	tpl:hasArgument _:target , _:source ;
	tpl:name "MOVE_PREDICATE" .

_:target a tpl:Argument ;
	tpl:displayName "Target predicate" ;
	tpl:mandatory "true" ;
	tpl:order "1"^^<http://www.w3.org/2001/XMLSchema#int> ;
	tpl:varName "target" .

_:source a tpl:Argument ;
	tpl:displayName "Source predicate" ;
	tpl:mandatory "true" ;
	tpl:order "0"^^<http://www.w3.org/2001/XMLSchema#int> ;
	tpl:varName "source" .